  "productName": "Dashborad",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "preelectron:run": "ELECTRON_START_URL=http://localhost:3000 || set ELECTRON_START_URL=http://localhost:3000",
    "electron:run": "electron .",
    "build": "node scripts/build.js",
    "postbuild": "purgecss --config purgecss.config.js",
    "package:android": "npm run build && npx cap copy android && node scripts/package.js && npx cap open android",
    "package:ios": "npm run build && npx cap copy ios && node scripts/package.js && capacitor build  ios",
    "package:win": "npm run build && node scripts/package.js && electron-builder    --win msi --x64",
    "package:mac": "npm run build && node scripts/package.js && electron-builder build --mac dmg --x64 --arm64",
    "package:linux": "npm run build && node scripts/package.js && electron-builder build --linux rpm deb tar.xz --x64",
    "test:unit": "node scripts/test.js --watchAll=false",
    "test:unit:watch": "node scripts/test.js",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:e2e": "npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:chrome": "nightwatch -e chrome --headless",
    "test:e2e:firefox": "nightwatch -e firefox --headless",
    "lighthouse": "lhci autorun",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint ./src/**/*.{css,scss,jsx,tsx}",
    "lint:js": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "format": "npm run format:js && npm run format:css",
    "format:css": "npm run lint:css -- --fix",
    "format:js": "npm run lint:js -- --fix",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "analyze": "node scripts/build.js --report"
  },
